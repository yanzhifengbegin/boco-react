{
    "compilerOptions": {
        "baseUrl": "./src",
        "strictNullChecks": true,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "jsx": "react",
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "noImplicitAny": false,
        "target": "es6",
        "lib": ["dom", "es2017"],
        "skipLibCheck": true
    },
    "exclude": ["node_modules", "lib", "es"],
    "compileOnSave": false
}
